{
  "name": "woaiso-system",
  "version": "0.0.2",
  "description": "woaiso system",
  "main": "./dist/server/www.js",
  "scripts": {
    "start": "node ./dist/server/www.js",
    "dev": "gulp",
    "build": "npm run webpack:build && gulp server:build",
    "webpack:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.ts --progress --colors",
    "webpack:build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.ts --progress --colors",
    "test": "jest --coverage"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathDirs": [
      "./src/client/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__spec__/.*\\.(ts|tsx|js)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woaiso/wst.git"
  },
  "keywords": [
    "woaiso",
    "template",
    "system"
  ],
  "author": "woaiso@woaiso.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/woaiso/wst/issues"
  },
  "homepage": "https://github.com/woaiso/wst#readme",
